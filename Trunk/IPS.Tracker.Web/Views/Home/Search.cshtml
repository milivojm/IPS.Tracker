@model List<IPS.Tracker.Web.TrackerService.DefectDTO>

@{
    ViewBag.Title = "Search results";
}

<h2>Search results</h2>

<div class="toolbox-container pull-left">
    
</div>

@if (Model.Count > 0)
{
    <span>You searched for term @ViewBag.SearchTerm.</span>
    @Html.Partial("ListProblems", Model)
}
else
{
    <div class="alert alert-warning" role="alert">No issues that fit the criteria found.</div>
}

