@model List<IPS.Tracker.Web.TrackerService.DefectDTO>

@{
    ViewBag.Title = "Rezultati pretraživanja";
}

<h2>Rezultati pretraživanja</h2>

<div class="toolbox-container pull-left">
    
</div>

@if (Model.Count > 0)
{
    <span>Tražili ste izraz @ViewBag.SearchTerm.</span>
    @Html.Partial("ListProblems", Model)
}
else
{
    <div class="alert alert-warning" role="alert">Nije pronađen niti jedan zadatak koji zadovoljava kriterij.</div>
}

